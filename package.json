    "next": "^10.0.1",
    "next-images": "^1.4.0",
    "postcss": "^8.1.7",
    "prop-types": "^15.7.2",
    "pure-react-carousel": "^1.27.6",
    "react": "^17.0.1",
    "react-cookie": "^4.0.3",
    "react-dom": "^16.13.1",
    "react-google-recaptcha": "^2.1.0",
    "react-hotkeys-hook": "^2.1.4",
    "react-icons": "^3.10.0",
    "react-input-mask": "^2.0.4",
    "react-loading-skeleton": "^2.1.1",
    "react-scroll": "^1.8.1",
