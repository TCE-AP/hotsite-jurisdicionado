    "@types/jest": "^24.0.0",
    "@types/node": "^14.0.20",
    "@types/react": "^16.9.41",
    "@types/react-google-recaptcha": "^2.1.0",
    "@types/react-input-mask": "^2.0.5",
    "@types/react-instantsearch": "^6.3.0",
    "@types/react-scroll": "^1.8.2",
    "@types/react-transition-group": "^4.4.0",
    "@types/yup": "^0.29.3",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
