{
  "name": "tceap-hotsite-jurisdicionado",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "tw:dev": "tailwind build ./src/styles/index.css -o ./src/styles/tailwind.css",
    "tw:build": "tailwind build ./src/styles/index.css -o ./src/styles/toUglify.css && uglifycss ./src/styles/toUglify.css > ./src/styles/tailwind.css && rm ./src/styles/toUglify.css",
    "dev": "next dev",
    "build": "yarn tw:build && next build",
    "start": "next start",
    "test": "next test",
    "test:coverage": "yarn test --coverage --watchAll false"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/pages/**/*.tsx",
      "src/components/**/*.tsx",
      "src/hooks/*.tsx",
      "!src/hooks/index.tsx"
    ]
  },
  "dependencies": {
